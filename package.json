{
  "name": "js-one-shot",
  "version": "1.0.5",
  "description": "Libraryの調査",
  "main": "entry.js",
  "author": "Himenon",
  "license": "MIT",
  "scripts": {
    "start": "run-p *:watch",
    "tsc:watch": "tsc -p tsconfig.json --watch",
    "express:session": "node ./lib/express-session/oneshot.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "yarn run lint -- --fix",
    "test": "jest -c jest.config.json --detectOpenHandles",
    "test:coverage": "yarn run test -- --coverage && cat ./coverage/lcov.info | npx coveralls",
    "test:watch": "yarn run test -- --watchAll --verbose false",
    "react:starter": "env DEBUG='*' webpack --config=./lib/react-starter/webpack.config.js --watch --mode=development",
    "version:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major",
    "version:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor",
    "version:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/travis-cli": "^7.2.1",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/hyperscript": "^0.0.3",
    "@types/jest": "^23.3.5",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.11.7",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.0.1",
    "@types/supertest": "^2.0.6",
    "@types/webpack": "^4.4.17",
    "coveralls": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^0.4.10",
    "generate-changelog": "^1.7.1",
    "gray-matter": "^4.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "js-yaml": "^3.12.0",
    "npm-run-all": "^4.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.5.2",
    "section-matter": "^1.0.0",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/standalone": "^7.1.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "hast-to-hyperscript": "^5.0.0",
    "html-whitespace-sensitive-tag-names": "^1.0.0",
    "hyperscript": "^2.0.2",
    "mdast-util-to-hast": "^3.0.2",
    "portfinder": "^1.0.17",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rehype-document": "^2.2.0",
    "rehype-format": "^2.3.0",
    "rehype-minify-whitespace": "^2.0.3",
    "rehype-parse": "^5.0.0",
    "rehype-react": "^3.0.3",
    "rehype-stringify": "^4.0.0",
    "remark": "^9.0.0",
    "remark-html": "^8.0.0",
    "remark-parse": "^6.0.1",
    "remark-react": "^4.0.3",
    "remark-rehype": "^3.0.1",
    "remark-slug": "^5.1.0",
    "remark-stringify": "^6.0.0",
    "styled-components": "^3.4.10",
    "to-vfile": "^5.0.2",
    "unified": "^7.0.0",
    "unified-stream": "^1.0.2",
    "vfile-reporter": "^5.1.0"
  }
}
